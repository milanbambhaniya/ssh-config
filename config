# Personal GitHub account:
Host github.com
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_rsa_personal
  PreferredAuthentications publickey
  PasswordAuthentication no
  IdentitiesOnly yes

# Work GitHub account:
Host git.ouroath.com
  HostName git.ouroath.com
  User git
  IdentityFile ~/.ssh/id_rsa
  PreferredAuthentications publickey
  PasswordAuthentication no
  IdentitiesOnly yes
  ForwardAgent yes
